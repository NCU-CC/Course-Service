server:
  port: 5278

logging:
  level: INFO

ssl:
  verification:
    enabled: false

custom:
  management:
    security:
      access: hasIpAddress('127.0.0.1') or hasIpAddress('0:0:0:0:0:0:0:1')
  connection:
    remote:
      prefix: https://localhost/Course/api/v0/
ncu:
  oauth:
    oauth-enabled: true
    server-path: https://localhost/oauth